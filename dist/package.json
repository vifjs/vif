{
    "name": "vifjs",
    "type": "module",
    "version": "0.1.0",
    "description": "A 5kb full featured reactive components library",
    "homepage": "https://vifjs.dev",
    "bugs": "https://github.com/vifjs/vif/issues",
    "author": {
        "name": "Enzo Aicardi",
        "url": "https://enzo.aicardi.pro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vifjs/vif"
    },
    "main": "./esm/vif.js",
    "module": "./esm/vif.js",
    "browser": "./esm/vif.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "require": "./cjs/vif.cjs",
            "import": "./esm/vif.js",
            "browser": "./esm/vif.js",
            "default": "./esm/vif.js"
        },
        "./esm": {
            "types": "./index.d.ts",
            "default": "./esm/vif.js"
        },
        "./cjs": {
            "types": "./index.d.ts",
            "default": "./cjs/vif.cjs"
        },
        "./umd": {
            "types": "./index.d.ts",
            "default": "./umd/vif.js"
        },
        "./iife": {
            "types": "./index.d.ts",
            "default": "./iife/vif.js"
        }
    },
    "keywords": [
        "javascript",
        "reactive",
        "hydration",
        "signal",
        "state",
        "revif",
        "components",
        "web-components",
        "es6",
        "attribute"
    ],
    "scripts": {
        "test": "echo \"No test specified\""
    },
    "license": "GPL-3.0-only"
}
